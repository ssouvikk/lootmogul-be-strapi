{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 250
    },
    "email": {
      "type": "email",
      "required": true,
      "maxLength": 200,
      "unique": true
    },
    "batchYear": {
      "type": "integer",
      "required": true,
      "max": 3000,
      "min": 1700
    },
    "colleges": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::college.college",
      "inversedBy": "students"
    },
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "inversedBy": "students"
    }
  }
}
